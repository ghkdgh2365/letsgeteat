<style>
  body{
    width:100%;height:100vh;
  }
  nav {     width:100%;
            height: 70px;
        line-height: 70px;
        
    }
  .material-icons {
     padding-top: 10px;
     padding-left: 20px;
  }
  
  a.button-collapse{
    text-decoration:none;
  }
  
  a.brand-logo {
    font-weight: bold;
    font-size: 25px;
    text-decoration:none;
  }
a:focus, a:hover{
  text-decoration:none;
}
#sidenav-overlay { margin-left: 900px; }
#sidenav-overlay { left: 900px; }
</style>
<body>
<div class="navbar-fixed">
  <nav>
      <div class="nav-wrapper">
        <a href="/home/index" class="brand-logo">공릉공릉</a>
        <a href="#" data-activates="mobile-demo" class="button-collapse"><i class="material-icons">menu</i></a>
        
          <ul class="right hide-on-med-and-down">
            <li><a href="/home/introduction">공릉공릉 소개</a></li>
            <li><a href="/home/index">처음으로</a></li>
            <li><a href="/home/search">검색</a></li>
            <li><a href="/home/registration">가게등록</a></li>
            <li><a href="/home/contact">문의</a></li>
            
            
            <% if user_signed_in? %>
              <ul id="dropdown1" class="dropdown-content">
                <li><%= link_to "마이페이지", edit_user_registration_path %></li>
                <li class="divider"></li>
                <li>
                  <%= link_to "로그아웃", destroy_user_session_path, method: :delete, data: { confirm: "Are you sure?" } %>
                </li>
              </ul>
            <% end %>
            <% if user_signed_in? %>
              <li><%= link_to "마이페이지", edit_user_registration_path %></li>
              <li><%= link_to "로그아웃", destroy_user_session_path, method: :delete, data: { confirm: "Are you sure?" } %></li>
              <% else %>
              <li>
                <%= link_to "로그인", new_user_session_path %>
              </li>
            <% end %>
          </ul>
          <ul class="side-nav" id="mobile-demo">
            <li><a href="/home/introduction">공릉공릉 소개</a></li>
            <li><a href="/home/index">처음으로</a></li>
            <li><a href="/home/search">검색</a></li>
            <li><a href="/home/registration">가게등록</a></li>
            <li><a href="/home/contact">문의</a></li>
            
            <% if user_signed_in? %>
              <li><%= link_to "마이페이지", edit_user_registration_path %></li>
              <li><%= link_to "로그아웃", destroy_user_session_path, method: :delete, data: { confirm: "Are you sure?" } %></li>
            <% else %>
              <li>
                <%= link_to "로그인", new_user_session_path %>
              </li>
            <% end %>
          </ul>
          
       </div>
  </nav>
</div>
<p class="notice"><%= notice %></p>

<%= yield %>

<script>
   $(".button-collapse").sideNav();
   

</script>


</body>
</html>
